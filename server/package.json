{
  "name": "doomed-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc --incremental",
    "start": "npm run build && node ./dist/index.js",
    "dev": "nodemon src/index.ts",
    "lint": "eslint './src/**/*.ts' --fix",
    "format": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
    "generate-migration": "npx drizzle-kit generate:pg --out src/db/migrations --schema=./src/db/schema",
    "migrate": "ts-node src/db/migrate"
  },
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.49.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.6.1",
    "@types/cors": "^2.8.14",
    "@types/http-status-codes": "^1.2.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "express": "^4.17.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "postgres": "^3.3.5",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  }
}
